    "typedoc-plugin-mdn-links": "^3.2.9",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "typedoc": "^0.26.0",
    "typedoc-plugin-merge-modules": "^6.0.0"
  }
