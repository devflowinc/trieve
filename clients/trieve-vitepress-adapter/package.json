    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "prepublishOnly": "yarn build",
    "dev": "tsc && node --env-file .env dist/index.js"
  },
  "license": "MIT",
  "homepage": "https://github.com/devflowinc/trieve/tree/main/clients/trieve-vitepress-adapter",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
