{
  "name": "trieve-vitepress-adapter",
  "version": "0.0.3",
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "private": false,
  "description": "Trieve adapter for Fern",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
